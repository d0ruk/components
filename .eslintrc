{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["react", "jsx-a11y"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "arrowFunctions": true,
      "blockBindings": true,
      "experimentalDecorators": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}